<Page xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      Background="#FF616161" Width="150">
    <StackPanel Margin="20">
        <TextBlock Text="This dockpanel function is work in progress. You can close it safely." Foreground="#FFFF6C6C" FontSize="20" TextWrapping="Wrap"  />
        <TextBlock Text="Select dim(s) and pick an action" Foreground="#FFCBCBCB" TextWrapping="Wrap" />
        <Button Content="Mark as FOG" Click="mark_FOG" HorizontalAlignment="Stretch" VerticalAlignment="Center" Height="50" Margin="0,10,0,0"/>
        <Button Content="Mark as EOS" Click="mark_EOS" HorizontalAlignment="Stretch" VerticalAlignment="Center" Height="50" Margin="0,10,0,0"/>
        <TextBlock x:Name="debug_textbox" Text="debug text" Foreground="#FFCBCBCB" />


    </StackPanel>
</Page>
